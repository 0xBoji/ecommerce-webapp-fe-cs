@model IEnumerable<ecommerce_webapp_fe_cs.Models.ProductModels.Product>

@foreach (var product in Model)
{
    <div>
        <h2>@product.ProName</h2>
        <p>@product.Description - Price: @product.Price</p>
        <a asp-controller="Product" asp-action="Details" asp-route-id="@product.ProId">View Details</a>
    </div>
}
